           <items>
               <TextFlow prefHeight="200.0" prefWidth="200.0">
                  <children>
                     <Text fx:id="ContentText" lineSpacing="3.0" strokeType="OUTSIDE" strokeWidth="0.0" text="You are in the Start Room. Open large clearing in the forest with a path leading to the &lt;East&gt;.">
                        <font>
                           <Font name="Cabin Sketch Regular" size="12.0" />
                        </font>
                     </Text>
                  </children>
               </TextFlow>
               <TextFlow prefHeight="200.0" prefWidth="200.0">
                  <children>
                     <Text fx:id="contentText1" strokeType="OUTSIDE" strokeWidth="0.0">
                        <font>
                           <Font name="Cabin Sketch Regular" size="11.0" />
                        </font></Text>
                  </children></TextFlow>
           </items>
         </SplitPane>
         <SplitPane dividerPositions="0.25" orientation="VERTICAL" prefHeight="200.0" prefWidth="160.0">
           <items>
               <TextFlow prefHeight="200.0" prefWidth="200.0">
