                              <font>
                                 <Font name="Cabin Sketch Regular" size="14.0" />
                              </font>
                           </Text>
                           <TextFlow prefHeight="15.0" prefWidth="70.0">
                              <children>
                                 <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Kills: ">
                                    <font>
                                       <Font name="Cabin Sketch Regular" size="12.0" />
                                    </font>
                                 </Text>
                                 <Text fx:id="killedTag" strokeType="OUTSIDE" strokeWidth="0.0" text="0 ">
