            <children>
               <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Through Worlds">
                  <font>
                     <Font name="Cabin Sketch Regular" size="36.0" />
                  </font>
                  <fill>
                     <LinearGradient endX="1.0" endY="1.0">
                        <stops>
                           <Stop>
                              <color>
                                 <Color red="0.26023390889167786" green="0.780701756477356" />
                              </color>
                           </Stop>
                           <Stop offset="1.0">
                              <color>
                                 <Color green="0.6499999761581421" blue="1.0" />
                              </color>
                           </Stop>
                        </stops>
                     </LinearGradient>
                  </fill>
               </Text>
            </children>
         </TextFlow>
         <TextFlow maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="30.0" prefWidth="450.0" textAlignment="CENTER">
