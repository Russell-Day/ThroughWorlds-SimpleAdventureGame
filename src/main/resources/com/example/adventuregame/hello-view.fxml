               <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Room Number: ">
                  <font>
                     <Font name="Cabin Sketch Regular" size="13.0" />
                  </font>
               </Text>
               <Text fx:id="RoomNumber" strokeType="OUTSIDE" strokeWidth="0.0" text="Start Room">
