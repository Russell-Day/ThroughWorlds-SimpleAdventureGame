                     <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Health:   ">
                        <font>
                           <Font name="Cabin Sketch Regular" size="18.0" />
                        </font>
                        <fill>
                           <LinearGradient endX="1.0" endY="1.0">
                              <stops>
                                 <Stop>
                                    <color>
                                       <Color red="0.6754385828971863" />
                                    </color>
                                 </Stop>
                                 <Stop offset="1.0">
