                     <Button fx:id="RunAway" mnemonicParsing="false" onAction="#onRunAwayButtonClick" text="Run Away" BorderPane.alignment="CENTER" />
                  </left>
                  <center>
                     <Button fx:id="Search" mnemonicParsing="false" onAction="#onSearchButtonClick" text="Search" BorderPane.alignment="CENTER" />
                  </center>
                  <right>
                     <Button fx:id="Sleep" mnemonicParsing="false" onAction="#onSleepButtonClick" text="Sleep" BorderPane.alignment="CENTER" />
                  </right></BorderPane>
            </center>
            <left>
               <Button fx:id="Fight" mnemonicParsing="false" onAction="#onFightButtonClick" text="Fight" BorderPane.alignment="CENTER">
